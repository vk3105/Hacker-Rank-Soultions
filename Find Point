/*
Logic : Mid point Theorm
*/
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int testCases;
    int px,py,qx,qy,hx,hy;
    
    scanf("%d",&testCases);
    
    while(testCases--)
    {
        scanf("%d%d%d%d",&px,&py,&qx,&qy);
        hx = 2*qx-px;
        hy = 2*qy-py;
        printf("%d %d\n",hx,hy);
    }
    
    return 0;
}
